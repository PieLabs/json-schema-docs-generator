{{> header}}
<div class="container">

	<div class="row">

		<div class="col-lg-9">
			{{#each page.sections}}
			<article id="{{id}}" class="row">

				{{#each schemas}}
				<section id="{{htmlID}}" class="col-lg-12">
          {{debug}}
					{{!-- object definition area --}}
          <h1>{{objectDefinition.title}}</h1>
          <h4>{{objectDefinition.description}}</h4>
					<section id="{{htmlID}}-properties">
						<div class="panel panel-default">

              {{debug}}
							{{#with objectDefinition.allProps}}
								{{> object-definition}}
							{{/with}}
						</div>
					</section>
				</section>
				{{/each}}
			</article>
			{{/each}}
		</div>
	</div>
</div>
{{> footer}}
